		<relativePath/>
	</parent>
	<groupId>com.gestion</groupId>
	<artifactId>productos</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>war</packaging>
	<name>productos</name>
	<description>Demo project for Spring Boot</description>
	<url/>
	<licenses>
		<license/>
